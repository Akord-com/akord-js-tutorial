{
  "name": "tutorial-vault-viewer",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@akord/akord-js": "5.0.3",
    "bootstrap": "^5.2.1",
    "eslint-config-react-app": "^7.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropdown": "^1.11.0",
    "react-markdown": "^8.0.3",
    "react-router-dom": "^6.4.0",
    "react-scripts": "4.0.3"
  },
  "scripts": {
    "start": "[[ \"$(node -v | cut -c 2-)\" < \"17\" ]] && react-scripts start || react-scripts --openssl-legacy-provider start",
    "build": "[[ \"$(node -v | cut -c 2-)\" < \"17\" ]] && react-scripts build || react-scripts --openssl-legacy-provider build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "clean": "rm -rf node_modules yarn-error.log yarn.lock"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      "chrome >= 67",
      "edge >= 79",
      "firefox >= 68",
      "opera >= 54",
      "safari >= 14"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "react-error-overlay": "6.0.9"
  }
}